{
  "name": "awp-backend",
  "version": "1.0.0",
  "description": "The backend for mini AI agent workflow platform app",
  "main": "./src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "test:unit": "vitest run --project unit --passWithNoTests",
    "test:integration": "node ./scripts/run-int-tests.js",
    "test": "npm run test:unit && npm run test:integration"
  },
  "author": "Ahmad Ghalib",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@repo/typescript-config": "*",
    "@repo/vitest-config": "*",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.39.1",
    "prisma": "^7.0.1",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  }
}
